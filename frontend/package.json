{
	"name": "grapheditor-frontend",
	"version": "0.0.0",
	"private": true,
	"author": "Mihajlo Lazar, Stefan Seliger",
	"license": "AGPL-3.0-or-later",
	"type": "module",
	"engines": {
		"node": ">=20",
		"npm": ">=10.0.0"
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"test": "npm run test:unit",
		"test:unit": "vitest",
		"test:coverage": "vitest run --coverage",
		"preview": "vite preview --port 8080",
		"lint": "eslint src --ext .ts --ext .tsx",
		"lint:fix": "eslint src --ext .ts --ext .tsx --fix",
		"prettier:fix": "prettier --write src",
		"dependencies:check": "ncu --peer",
		"dependencies:update": "ncu --peer --upgrade --install always",
		"generate-icon-font": "node ./scripts/generate-icon-font.js"
	},
	"dependencies": {
		"@db-ux/react-core-components": "^3.0.7",
		"@floating-ui/react": "^0.27.15",
		"@sigma/edge-curve": "^3.1.0",
		"@sigma/node-border": "^3.0.0",
		"@sigma/node-square": "^3.0.0",
		"@sigma/utils": "^3.0.0",
		"axios": "^1.11.0",
		"clsx": "^2.1.1",
		"graphology": "^0.26.0",
		"graphology-layout": "^0.6.1",
		"graphology-layout-force": "^0.2.4",
		"graphology-layout-forceatlas2": "^0.10.1",
		"graphology-layout-noverlap": "^0.4.2",
		"i18next": "^25.3.6",
		"i18next-browser-languagedetector": "^8.2.0",
		"jsdom": "^26.1.0",
		"lucide-react": "^0.539.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-hook-form": "^7.62.0",
		"react-i18next": "^15.6.1",
		"react-markdown": "^10.1.0",
		"react-router-dom": "^7.8.1",
		"seedrandom": "^3.0.5",
		"sigma": "^3.0.2",
		"source-map-js": "^1.2.1",
		"yup": "^1.7.0",
		"zustand": "^5.0.7"
	},
	"devDependencies": {
		"@cyclonedx/cyclonedx-npm": "^4.0.0",
		"@db-ux/icon-font-tools": "^0.3.4",
		"@types/hoist-non-react-statics": "^3.3.7",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"@types/react-router-dom": "^5.3.3",
		"@types/seedrandom": "^3.0.8",
		"@types/ws": "^8.18.1",
		"@typescript-eslint/eslint-plugin": "^8.39.1",
		"@typescript-eslint/parser": "^8.39.1",
		"@vitejs/plugin-react": "^5.0.0",
		"@vitest/browser": "^3.2.4",
		"@vitest/coverage-v8": "^3.2.4",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-config-xo": "^0.48.0",
		"eslint-plugin-prettier": "^5.5.4",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-simple-import-sort": "^12.1.1",
		"license-compliance": "^3.0.1",
		"lucide-static": "^0.539.0",
		"msw": "^2.10.5",
		"npm-check-updates": "^18.0.2",
		"playwright": "^1.54.2",
		"playwright-chromium": "^1.54.2",
