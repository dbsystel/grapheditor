		"vitest": "^3.2.4",
		"vitest-browser-react": "^1.0.1"
	},
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
